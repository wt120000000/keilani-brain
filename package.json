{
  "name": "keilani-brain",
  "private": true,
  "type": "commonjs",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "glob": "^11.0.3",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "netlify-cli": "^23.5.1",
    "prettier": "^3.6.2"
  },
  "scripts": {
    "dev": "netlify dev",
    "build": "echo \"Nothing to build\"",
    "deploy": "netlify deploy --prod",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.mjs --max-warnings=0",
    "prepare": "husky"
  },
  "lint-staged": {
    "netlify/functions/**/*.js": [
      "eslint --fix"
    ],
    "scripts/**/*.js": [
      "eslint --fix"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  }
}
