{
  "name": "keilani-brain",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@netlify/functions": "^2.8.1",
    "@supabase/supabase-js": "^2.56.1",
    "glob": "^11.0.3",
    "stripe": "^18.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "netlify-cli": "^17.33.1",
    "prettier": "^3.6.2",
    "tsx": "^4.19.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "netlify dev --port 3000 --staticServerPort 3999",
    "build": "tsc --noEmit && npm run validate-env",
    "typecheck": "tsc --noEmit",
    "validate-env": "tsx scripts/validate-env.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "netlify deploy --prod",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.mjs,.ts --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.mjs,.ts --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "netlify/functions/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "scripts/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "lib/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  }
}
